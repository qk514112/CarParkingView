                    android:id="@+id/forward_right_ten"
                    android:layout_width="27dp"
                    android:layout_height="137dp"
                    android:visibility="invisible"
                    android:src="@drawable/forward_right_ten"
                    android:layout_gravity="bottom"/>
            </FrameLayout>

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <Button
            android:id="@+id/forward_turn_left"
            android:text="左进"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            />

        <Button
            android:id="@+id/forward_turn_right"
            android:text="右进"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            />
        <Button
            android:id="@+id/forward_mid_left"
            android:text="进中左"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            />
        <Button
            android:id="@+id/forward_mid_right"
            android:text="进中右"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            />
    </LinearLayout>

</LinearLayout>